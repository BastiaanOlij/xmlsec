<programlisting><link linkend="xmlNodePtr"><returnvalue>xmlNodePtr</returnvalue></link>          xmlSecSoap12GetFaultEntry           (<parameter><link linkend="xmlNode"><type>xmlNodePtr</type></link> envNode</parameter>);</programlisting>
