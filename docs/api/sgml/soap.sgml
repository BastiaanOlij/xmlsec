<programlisting><link linkend="xmlNodePtr"><returnvalue>xmlNodePtr</returnvalue></link>          xmlSecSoap11AddBodyEntry            (<parameter><link linkend="xmlNode"><type>xmlNodePtr</type></link> envNode</parameter>,
