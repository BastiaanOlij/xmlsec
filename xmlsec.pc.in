prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: xmlsec
Version: @VERSION@
Description: XML Security Library implements XML Signature and XML Encryption standards
Requires: libxml-2.0 >= @LIBXML_MIN_VERSION@ libxslt >= @LIBXSLT_MIN_VERSION@ 
Cflags: -I${includedir}/xmlsec  @XMLSEC_CRYPTO_CFLAGS@ @LIBXSLT_CFLAGS@ @LIBXML_CFLAGS@ @CFLAGS@
Libs: -L${libdir} -lxmlsec -l@XMLSEC_CRYPTO_LIB@  @XMLSEC_CRYPTO_LIBS@ @LIBXSLT_LIBS@ @LIBXML_LIBS@ @LIBS@
